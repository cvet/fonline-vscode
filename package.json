{
	"name": "fonline-vscode",
	"displayName": "FOnline",
	"description": "FOnline VSCode Extension",
	"keywords": [
		"fonline"
	],
	"author": {
		"name": "cvet",
		"email": "cvet@tut.by",
		"url": "https://fonline.ru"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cvet/fonline-vscode.git"
	},
	"publisher": "cvet",
	"version": "0.3.2",
	"license": "MIT",
	"preview": true,
	"icon": "resources/radiation.png",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:fonlineActions"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "FOnline",
			"properties": {
				"fonline.path": {
					"type": "string",
					"description": "Path to the FOnline Engine repository",
					"scope": "resource"
				},
				"fonline.cmake": {
					"type": "string",
					"description": "Path to the CMake contribution file",
					"scope": "resource"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fonline",
					"title": "FOnline",
					"icon": "resources/activity.png"
				}
			]
		},
		"views": {
			"fonline": [
				{
					"id": "fonlineActions",
					"name": "Actions"
				}
			]
		}
	},
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish patch",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mkdirp": "^1.0.0",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.27",
		"@types/rimraf": "^2.0.3",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.19.2",
		"@typescript-eslint/parser": "^2.19.2",
		"eslint": "^8.2.0",
		"glob": "^7.1.6",
		"mocha": "^9.1.3",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"jqpropertygrid": "^1.0.0",
		"jquery": "^3.5.0",
		"jqueryui": "^1.11.1",
		"mkdirp": "^1.0.3",
		"rimraf": "^3.0.2",
		"uikit": "^3.3.1"
	}
}
